  "bugs": {
    "url": "https://github.com/CalebeCopello/GeekShop/issues"
  },
  "homepage": "https://github.com/CalebeCopello/GeekShop#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
